
<% if (theme.socialLink.dingtalk.enable) { %>
    <a href="<%= theme.socialLink.dingtalk.img %>"  class="tooltipped"
       target="_blank" data-tooltip="钉钉扫码加入公益电子技术入门直播群（同公益课直达）" data-position="top" data-delay="50">
       <i class="fas fa-thumbtack"></i>
    </a>
<% } %>

<% if (theme.socialLink.wechat.enable) { %>
    <a href="<%= theme.socialLink.wechat.img %>"  class="tooltipped"
       target="_blank" data-tooltip="微信扫码加知乎向导的微信" data-position="top" data-delay="50">
       <i class="fab fa-weixin"></i>
    </a>
<% } %>

<% if (theme.socialLink.zhihu) { %>
    <a href="<%= theme.socialLink.zhihu %>" class="tooltipped" target="_blank" data-tooltip="关注向导的知乎" data-position="top" data-delay="50">
        <i class="fab fa-zhihu1">知</i>
    </a>
<% } %>

<% if (theme.socialLink.weixingzh.enable) { %>
    <a href="<%= theme.socialLink.weixingzh.img %>" class="tooltipped" target="_blank" data-tooltip="微信扫描加向导公众号" data-position="top" data-delay="50">
        <i class="fas fa-comments"></i>
    </a>
<% } %>

<% if (theme.socialLink.github) { %>
    <a href="<%= theme.socialLink.github %>" class="tooltipped" target="_blank" data-tooltip="访问我的GitHub" data-position="top" data-delay="50">
        <i class="fab fa-github"></i>
    </a>
<% } %>

<% if (theme.socialLink.email) { %>
    <a href="https://mail.qq.com/cgi-bin/qm_share?t=qm_mailme&email=<%= theme.socialLink.email %>" class="tooltipped"
       target="_blank" data-tooltip="邮件联系我" data-position="top" data-delay="50">
       <i class="fas fa-envelope-open"></i>
    </a>
<% } %>

<% if (theme.socialLink.facebook) { %>
    <a href="<%= theme.socialLink.facebook %>" class="tooltipped" target="_blank" data-tooltip="关注我的Facebook: <%= theme.socialLink.facebook %>" data-position="top" data-delay="50">
        <i class="fab fa-facebook-f"></i>
    </a>
<% } %>

<% if (theme.socialLink.twitter) { %>
    <a href="<%= theme.socialLink.twitter %>" class="tooltipped" target="_blank" data-tooltip="关注我的Twitter: <%= theme.socialLink.twitter %>" data-position="top" data-delay="50">
        <i class="fab fa-twitter"></i>
    </a>
<% } %>

<% if (theme.socialLink.qq) { %>
    <a href="tencent://AddContact/?fromId=50&fromSubId=1&subcmd=all&uin=<%= theme.socialLink.qq %>" class="tooltipped" target="_blank" data-tooltip="QQ联系我（不常用，建议微信联系）: <%= theme.socialLink.qq %>" data-position="top" data-delay="50">
        <i class="fab fa-qq"></i>
    </a>
<% } %>

<% if (theme.socialLink.weibo) { %>
    <a href="<%= theme.socialLink.weibo %>" class="tooltipped" target="_blank" data-tooltip="关注我的微博" data-position="top" data-delay="50">
        <i class="fab fa-weibo"></i>
    </a>
<% } %>


<% if (theme.socialLink.rss) { %>
    <a href="<%- url_for('/atom.xml') %>" class="tooltipped" target="_blank" data-tooltip="RSS 订阅" data-position="top" data-delay="50">
        <i class="fas fa-rss"></i>
    </a>
<% } %>
